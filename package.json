{
  "name": "peach-math-field",
  "version": "0.0.1",
  "description": "A shadcn-native, highly customizable math editor",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "katex": "^0.16.21"
  },
  "devDependencies": {
    "@types/katex": "^0.16.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "math",
    "editor",
    "latex",
    "katex",
    "react",
    "shadcn",
    "wysiwyg"
  ],
  "author": "peach-math-field contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peach-math-field/peach-math-field.git"
  },
  "bugs": {
    "url": "https://github.com/peach-math-field/peach-math-field/issues"
  },
  "homepage": "https://github.com/peach-math-field/peach-math-field#readme"
}
